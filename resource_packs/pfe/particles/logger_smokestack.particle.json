{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "poke:logger_smokestack",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particles/PFE_particles"
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 5,
				"max_particles": 400
			},
			"minecraft:emitter_lifetime_looping": {
				"active_time": 1
			},
			"minecraft:emitter_shape_point": {
				"direction": ["q.body_x_rotation", 1, "-q.body_x_rotation"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 4
			},
			"minecraft:particle_initial_spin": {
				"rotation": 90,
				"rotation_rate": 35
			},
			"minecraft:particle_initial_speed": 1.25,
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, 1.5, 0],
				"linear_drag_coefficient": 0.25,
				"rotation_acceleration": 2,
				"rotation_drag_coefficient": 0.1
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["0.5 * v.particle_age / v.particle_lifetime", "0.5 *  v.particle_age / v.particle_lifetime"],
				"facing_camera_mode": "lookat_xyz",
				"uv": {
					"texture_width": 128,
					"texture_height": 128,
					"uv": ["Math.floor(v.particle_random_3 * 2) * 8", "88+Math.floor(v.particle_random_4 * 2) * 8"],
					"uv_size": [8, 8]
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#FF2A2A2A",
						"1.0": "#26FFFFFF"
					}
				}
			}
		}
	}
}