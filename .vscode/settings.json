{
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "[typescriptreact]": {
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.formatOnSave": true,
    "prettier.embeddedLanguageFormatting": "off"
  },
  "git.ignoreLimitWarning": true,
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/lib": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "cSpell.words": [
    "boltbow",
    "eastside",
    "gametest",
    "gametests",
    "Haxels",
    "mcaddon",
    "mcpedl",
    "minecart",
    "modbay",
    "netherrack",
    "northside",
    "southside",
    "stackable",
    "westside"
  ],
  "editor.tabSize": 2,
  "eslint.experimental.useFlatConfig": true
}