				"is_pushable": true,
				"is_pushable_by_piston": true
			}
		},
		"events": {
			"poke:spawn": {
				"queue_command": {
					"command": "say Hello World"
				},
				"randomize": [
					{
						"queue_command": {
							"command": [
								"execute as @s run summon zombie ~~~",
								"execute as @s run summon zombie ~~~"
							]
						},
						"add": {
