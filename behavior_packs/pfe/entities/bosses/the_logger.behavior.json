			"identifier": "poke:the_logger",
			"is_spawnable": true,
			"is_summonable": true,
			"is_experimental": false
			
		},
		"component_groups": {
			"timer":{
				"minecraft:timer": {
					"time": 1800,
					"looping": true,
					"time_down_event": {
						"event": "poke:despawn",
						"target": "self"
					}
				}
			},
			"despawn":{
				"minecraft:instant_despawn": {
					"remove_child_entities": true
				}
			}
		},
		"components": {
			"minecraft:behavior.hurt_by_target": {
				"priority": 1
			},
			"minecraft:behavior.nearest_attackable_target": {
				"priority": 2,
				"within_radius": 55,
				"reselect_targets": true,
				"entity_types": [
					{
						"filters": {
							"any_of": [
								{
									"test": "is_family",
									"subject": "other",
									"value": "player"
								}
							]
						},
						"max_dist": 35
					}
				]
			},
			"minecraft:behavior.delayed_attack": {
				"priority": 0,
				"attack_once": false,
				"track_target": true,
				"require_complete_path": false,
				"reach_multiplier": 2,
				"speed_multiplier": 2,
				"attack_duration": 0.5,
				"hit_delay_pct": 0.75,
				"melee_fov": 180
			},
			"minecraft:knockback_resistance": {
				"value": 1
			},
			"minecraft:loot":{
				"table": "loot_tables/poke/pfe/logger.json"
			},
			"minecraft:physics": {
				"has_gravity": true,
				"has_collision": true
			},
			"minecraft:pushable": {
				"is_pushable": false
			},
			"minecraft:movement": {
				"value": 0.25
			},
			"minecraft:behavior.random_stroll": {
				"priority": 8,
				"speed_multiplier": 0.8
			},
			"minecraft:custom_hit_test": {
				"hitboxes": [
					{
						"width": 2.2,
						"height": 4.5,
						"pivot": [0, 2.25, 0]
					}
				]
			},
			"minecraft:boss": {
				"hud_range": 55,
				"name": "entity.poke:the_logger.name",
				"should_darken_sky": false
			},
			"minecraft:follow_range": {
				"value": 55
			},
			"minecraft:health": {
				"value": 7500,
				"max": 7500
			},
			"minecraft:attack": {
				"damage": 75
			},
			"minecraft:attack_damage": {
				"value": 75,
				"max": 80
			},
			"minecraft:behavior.look_at_player": {
				"priority": 10,
				"look_distance": 25,
				"target_distance": 6
			},
			"minecraft:behavior.random_look_around": {
				"priority": 15
			},
			"minecraft:type_family": {
				"family": [
					"poke:the_logger",
					"monster",
					"mob",
					"pfe:boss"
				]
			},
			"minecraft:fire_immune": {},
			"minecraft:navigation.walk": {
				"can_path_over_water": true,
				"can_float": true
			},
			"minecraft:movement.basic": {},
			"minecraft:jump.static": {
				"jump_power": 0.5
			},
			"minecraft:can_climb": {},
			"minecraft:damage_sensor": {
				"triggers": [
					{
						"cause": "suffocation",
						"deals_damage": "no"
					},
					{
						"cause": "fall",
						"deals_damage": "no"
					},
					{
						"cause": "fire",
						"deals_damage": "no"
					}
				]
			},
			"minecraft:experience_reward": {
				"on_death": 5000
			},
			"minecraft:nameable": {
				"name_actions":{
					"name_filter": "despawn",
					"on_named": {
						"event": "poke:despawn",
						"target": "self"
					}
				},
				"default_trigger": {
					"target": "self",
					"event": "poke:renamed"
				}
